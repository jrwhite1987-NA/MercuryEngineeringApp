<Application x:Class="MercuryEngApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/StyleDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <XmlDataProvider x:Key="XMLData" Source="LocalFolder/InfoConfig.xml" XPath="ProbePartNumbers" IsInitialLoadEnabled="True"></XmlDataProvider>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
